/* pages/my/my.wxss */
@import "../../font/iconfont.wxss";
page{
  background: #F2F2F2;
}
.avatar {
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  overflow: hidden;
}
.nav-warp {
  background: #fff;
}
.nav-warp .nav-item {
  padding: 40rpx;
  display: flex;
  justify-content: space-between;
}
.jishu{
  width: 100%;
  text-align: center;
  font-size: 34rpx;
  margin-top: 20rpx;
  padding: 40rpx 0;
}
.jishu text{
  border-bottom: 1px solid #333;
  padding: 10rpx 0;
}
.orderItem{
  width: 25%;
}
.orderItem i.iconfont{
  font-size: 44rpx;
}
.getUserInfo{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  opacity: 0;
}
.avatar_info{
  width:140rpx;
  height:140rpx;
  border-radius:50%;
}
.integral_item{
  width: 50%;
  box-sizing: border-box;
  text-align: center;
  padding: 30rpx 0;
}
.c_f9d{
  color: #F9D1E7;
}
.c_fdb{
  color: #FDBD3F;
}
.c_f68{
  color: #F68136;
}
.c_ff3{
  color: #FF3248;
}
.c_71b{
  color: #71B1E9;
}
.my_footer{
 flex-wrap: wrap;
}
.my_footer .nav-item .iconfont{
  font-size: 46rpx;
}
.small{
  width: 60rpx;
  height: 60rpx;
}
.my_h{
  position: absolute;
  top: 0;
  left: 0;
}
.my_t{
  z-index: 100;
  color: #fff;
  position: relative;
}
.distribution{
  position: fixed;
  width: 100%;
  height: 100vh;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.3);
  z-index: 1000;
}
.disBody{
  width: 70%;
  margin: 80rpx auto 0;
  border-radius: 20rpx;
  padding: 40rpx;
}
.disInput{
  border-radius: 20rpx;
  border: 1px solid #333;
  width: 70%;
  height: 70rpx;
  line-height: 70rpx;
  padding: 0 10rpx;
}
.disHeader{
  margin-bottom: 50rpx;
  font-size: 38rpx;
}
.s_foot{
  border-radius: 20rpx;
  margin-top: 40rpx;
}
.icon-guanbi{
  position: absolute;
  top: 40rpx;
  right: 40rpx;
}